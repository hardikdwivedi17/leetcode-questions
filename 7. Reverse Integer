class Solution {
    public int reverse(int x) {
        long rev=0;
        int h=0;
       
            while(x!=0)
            {
            int n=x%10;
            rev=rev*10+n;
            x=x/10;
            }
            if(rev<Integer.MAX_VALUE && rev>Integer.MIN_VALUE)
            return (int)rev;
            else return 0;

       

    }
}
