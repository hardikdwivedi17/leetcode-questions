class Solution {
    public boolean isHappy(int n) {
        boolean x=false;
        
        
        while(n>0)
        {int sum=0;
        while(n>0)
        {
            sum+=(n%10)*(n%10);
            n=n/10;
            
        }
        n=sum;
        if(n==1)
        return true;
        if(n==4)
        return false;
         
        }
      return x;  
    }
}
