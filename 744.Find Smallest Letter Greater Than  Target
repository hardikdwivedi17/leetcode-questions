class Solution {
    public char nextGreatestLetter(char[] letters, char target) {
        int tar=target;
        int ab=letters[0];
        char c;
        int i=0;
        for(i=0;i<letters.length;i++)
        {
            int abs=letters[i];
            if(tar<abs)
            return letters[i];

        }
      c=(char)ab;  
      return c;  
        
    }
}
