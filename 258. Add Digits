class Solution {
    public int addDigits(int num) {
        int h=num;
        while(num>9)
        {int sum=0;
        while(num>0)
        {
            sum+=num%10;
            num/=10;
        }
        num=sum;
        if(num<=9)
        return num;
        }
        

        return h;
    }
}
